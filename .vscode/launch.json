{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "gdb",
      "request": "attach",
      "remote": true,
      "target": ":1234",
      "executable": "${workspaceFolder}/vm/build/kernel.o",
      "cwd": "${workspaceFolder}/vm/build",
      "valuesFormatting": "parseText",
      "autorun": [
        "set arch i386:x86-64:intel",
        "break main",
        "continue",
        "disconnect",
        "set arch i386:x86-64",
        "target remote localhost:1234",
        "break init_thread"
      ]
    }
  ]
}
